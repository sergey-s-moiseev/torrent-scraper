{
  "name": "sergey-s-moiseev/torrent-scraper",
  "description": "Provide an abstraction to seach for torrent files on many websites.",
  "keywords": [
    "torrent",
    "piratebay",
    "kickass"
  ],
  "license": "MIT",
  "homepage": "https://github.com/sergey-s-moiseev/torrent-scraper",
  "authors": [
    {
      "name": "Fernando Carletti",
      "email": "contato@fernandocarletti.net",
      "homepage": "http://fernandocarletti.net",
      "role": "Developer"
    },
    {
      "name": "Sergey S. Moiseev",
      "email": "sergey.s.moiseev@gmail.com",
      "homepage": "http://www.moses-team.ru",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.5",
    "ext-curl": "*",
    "guzzlehttp/guzzle": "~6.2",
    "symfony/dom-crawler": ">=2.6",
    "symfony/css-selector": ">=2.6",
    "kleiram/transmission-php": "dev-master"

  },
  "require-dev": {
    "phpunit/phpunit": "5.2.*",
    "symfony/var-dumper": ">=2.6",
    "squizlabs/php_codesniffer": "2.*"
  },
  "autoload": {
    "psr-4": {
      "SergeySMoiseev\\TorrentScraper\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SergeySMoiseev\\TorrentScraper\\": "tests"
    }
  }
}
